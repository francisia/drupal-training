<?php

/**
 * @file
 * Module file for the central module, which carries any global site
 * configuration settings.
 */

/**
 * Implements hook_help().
 *
 * Displays help and module information.
 */
function central_help($path, $arg) {
  if ($path == "admin/help#central") {
    return t("Carries any global site configuration settings.") . '</p>';
  }
}
